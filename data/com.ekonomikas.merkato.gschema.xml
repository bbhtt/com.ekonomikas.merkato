<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="merkato">
	<schema id="com.ekonomikas.merkato" path="/com/ekonomikas/merkato/">
    <key type="i" name="pull-interval">
      <default>30</default>
      <summary>Pull interval</summary>
      <description>Pull data every X seconds.</description>
    </key>
    <key type="b" name="dark-theme">
      <default>true</default>
      <summary>Use dark GTK+ theme</summary>
      <description>Use the dark variant of your GTK+ theme. Please note that not every GTK+ theme has a dark variant.</description>
    </key>
    <key type="i" name="window-width">
      <default>300</default>
      <summary>Window width</summary>
      <description>Remember the window width.</description>
    </key>
    <key type="i" name="window-height">
      <default>600</default>
      <summary>Window height</summary>
      <description>Remember the window height.</description>
    </key>
    <key type="i" name="pref-window-width">
      <default>400</default>
      <summary>Window preferencies width</summary>
      <description>Remember the window width.</description>
    </key>
    <key type="i" name="pref-window-height">
      <default>600</default>
      <summary>Window preferencies height</summary>
      <description>Remember the window height.</description>
    </key>
    <key type="i" name="order-view-ref">
      <default>0</default>
      <summary>Order symbol view</summary>
      <description>Remember the order view.</description>
    </key>
  </schema>
</schemalist>  
